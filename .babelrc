{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    ["import", {
    "libraryName": "antd",
    "libraryDirectory": "es",
    "style": "css"
  }]],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [{
              "transform": "react-transform-hmr",
              "imports": [
                "react"
              ],
              "locals": [
                "module"
              ]
            }]
          }
        ]
      ]
    }
  }
}